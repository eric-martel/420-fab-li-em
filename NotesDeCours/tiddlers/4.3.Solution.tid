// Définit un tableau (liste) des jours de la semaine
string[] joursSemaine = { "Dimanche", "Lundi", "Mardi", "Mercredi", "Jeudi", "Vendredi", "Samedi" };

Console.WriteLine("Les jours de la semaine sont:");

// Pour chaque élément du tableau...
foreach (string jour in joursSemaine)
{
    Console.WriteLine($"\t{jour}");
}
